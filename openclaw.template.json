{
  "commands": {
    "native": "auto",
    "nativeSkills": "auto",
    "restart": true,
    "ownerDisplay": "raw"
  },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "port": 18789,
    "auth": {
      "mode": "token",
      "token": "replace_with_openclaw_gateway_token"
    },
    "controlUi": {
      "enabled": true,
      "basePath": "/openclaw",
      "allowInsecureAuth": true,
      "allowedOrigins": [
        "http://your_server_ip_or_domain:18789"
      ],
      "dangerouslyDisableDeviceAuth": true
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  "agents": {
    "defaults": {
      "models": {
        "bailian-us/MiniMax-M2.5": { "alias": "minimax" },
        "bailian-us/qwen3-coder-plus": { "alias": "qwen" }
      },
      "model": {
        "primary": "bailian-us/MiniMax-M2.5"
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "bailian-us": {
        "baseUrl": "https://coding.dashscope.aliyuncs.com/v1",
        "apiKey": "replace_with_bailian_sk_sp_key",
        "api": "openai-completions",
        "models": [
          { "id": "MiniMax-M2.5", "name": "MiniMax-M2.5" },
          { "id": "qwen3-coder-plus", "name": "qwen3-coder-plus" }
        ]
      }
    }
  }
}
